{
  "family": "zi-wei-dou-shu",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "2048",
  "taskRoleArn": "arn:aws:iam::502266988632:role/ecsTaskExecutionRole",
  "executionRoleArn": "arn:aws:iam::502266988632:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "zi-wei-dou-shu-gin",
      "image": "502266988632.dkr.ecr.ap-southeast-1.amazonaws.com/zi-wei-dou-shu-gin:lasted",
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ],
      "cpu": "512",
      "memory": "1024",
      "essential": true,
      "command": [
        "wget https://zi-wei-dou-shu-config.s3-ap-southeast-1.amazonaws.com/env.yaml --output-document=$WORKDIR/configs/yaml/release-config.yml",
        "zi-wei-dou-shu-gin"
      ]
    }
  ]
}
